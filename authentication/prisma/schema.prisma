// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model User {
  id String @id @default(uuid())
  FirstName String?
  LastName String?
  Phone Int?
  UserName String?
  Email String @unique
  password String
}